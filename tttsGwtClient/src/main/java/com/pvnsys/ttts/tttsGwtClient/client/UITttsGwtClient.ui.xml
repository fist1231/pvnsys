<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel height="90%" width="100%">
			<g:TextBox ui:field="search" width="218px"/>
			<g:Button styleName="{style.important}" ui:field="button" />
			<p1:CellTable ui:field="cellTable" width="100%" height="182px" autoFooterRefreshDisabled="true" autoHeaderRefreshDisabled="true"/>
			<p1:SimplePager ui:field="pager" location="CENTER" width="100%"/>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 